{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "lint": {
      "outputs": []
    },
    "test": {
      "inputs": ["test/**"]
    },
    "test:e2e": {
      "dependsOn": ["^test"],
      "inputs": ["test/**"]
    },
    "test:cov": {
      "dependsOn": ["^test:e2e"],
      "inputs": ["test/**"]
    },
    "dev": {
      "cache": false
    }
  }
}
